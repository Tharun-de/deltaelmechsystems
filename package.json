{
  "name": "delta-elmech-systems-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:ci": "start-server-and-test dev http://localhost:3000 test:e2e",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "prepare": "husky install",
    "db:migrate": "node supabase/migrate.js",
    "db:migrate:create": "node -e \"const fs=require('fs');const path=require('path');const name=process.argv[1];const timestamp=new Date().toISOString().replace(/[^0-9]/g, '').slice(0,14);const filename=path.join('supabase/migrations', `${timestamp}_${name}.sql`);fs.writeFileSync(filename, '-- Migration SQL here');console.log(`Created migration: ${filename}`);\" --",
    "db:seed": "node supabase/seed.js",
    "start:dev": "concurrently \"npm run dev\" \"cd backend && npm run dev\"",
    "start:prod": "cd backend && npm run start",
    "build:all": "npm run build && cd backend && npm run build",
    "deploy:setup": "npm install && cd backend && npm install",
    "deploy:frontend": "vercel --prod",
    "deploy:backend": "railway up",
    "deploy": "npm run build:all && npm run deploy:frontend && npm run deploy:backend"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@sentry/react": "^7.107.0",
    "@sentry/tracing": "^7.107.0",
    "@stripe/react-stripe-js": "^2.5.0",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.39.8",
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-query-devtools": "^5.28.4",
    "axios": "^1.6.7",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.1",
    "framer-motion": "^11.0.8",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.54.2",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2",
    "swiper": "^11.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@shadcn/ui": "^0.0.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "cypress": "^13.6.6",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "sharp": "^0.33.5",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,scss}": [
      "prettier --write"
    ]
  }
}
